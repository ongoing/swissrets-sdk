        xsi:noNamespaceSchemaLocation="https://swissrets.ch/dist/v2.7.0/schema.xsd">
    <created>{{ 'now' | date("Y-m-d\TH:i:s") }}</created>
    <generator version="{{ exportDto.generatorVersion }}">{{ exportDto.generatorName }}</generator>
    <projects>
{#        {% for projectDto in exportDto.getProjects %}#}
